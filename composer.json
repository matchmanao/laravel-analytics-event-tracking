{
    "name": "matchmanao/laravel-analytics-event-tracking",
    "description": "Laravel package to easily send events to Google Analytics",
    "keywords": [
        "matchmanao",
        "protonemedia",
        "laravel",
        "google-analytics",
        "laravel-analytics",
        "analytics",
        "laravel-analytics-event-tracking",
        "analytics-event-tracking"
    ],
    "homepage": "https://github.com/matchmanao/laravel-analytics-event-tracking",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "role": "Developer"
        },
        {
            "name": "Stefan Kreuter",
            "email": "stefan@matchmanao.de",
            "role": "Developer"
        },
        {
            "name": "Jan Hohner",
            "email": "jan@matchmanao.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/bus": "^6.0 || ^7.0 || ^8.0",
        "illuminate/queue": "^6.0 || ^7.0 || ^8.0",
        "illuminate/http": "^6.0 || ^7.0 || ^8.0",
        "illuminate/validation": "^6.0 || ^7.0 || ^8.0",
        "theiconic/php-ga-measurement-protocol": "^2.7"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\AnalyticsEventTracking\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\AnalyticsEventTracking\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ProtoneMedia\\AnalyticsEventTracking\\ServiceProvider"
            ]
        }
    }
}
